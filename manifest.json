{
    "name": "Enable right click",
    "version": "0.0.5",
    "manifest_version": 2,
    "default_locale": "en",
    "icons": {
        "48": "img/icon48.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "all_frames": true,
            "js" : [
                "src/content_scripts.js"
            ]
        }
    ]
}